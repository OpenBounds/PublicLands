{
  "name": "State Preserves in Kansas",
  "attribution": "Kansas Natural Heritage Inventory",
  "properties": {
    "adminAgency": "MANAGER",
    "name": "AREA_NAME",
    "adminLevel": {"static": "state"},
    "type" : {"static": "preserve"}
  },
  "filetype": "shp",
  "url": "http://data.kansasgis.org/environmental_resources/shp/protected_areas/protected_areas.zip",
  "filter": [
    {
      "expression": "match",
      "key": "MANAGER",
      "value": "(Kansas Dept. of Wildlife and Parks|University of Kansas)"
    },
    {
      "expression": "match",
      "key": "AREA_NAME",
      "value": ".*PRESERVE"
    }
  ]
}
